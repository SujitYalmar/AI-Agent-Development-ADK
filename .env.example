# Environment Configuration for AI Agent Development
# Copy this file to .env and fill in your actual values
# WARNING: Never commit .env file to version control!

# ============================================
# Google API Configuration
# ============================================

# Get your API key from: https://aistudio.google.com/app/api-keys
GOOGLE_API_KEY=your_api_key_here

# Set to FALSE when using Gemini API (not Vertex AI)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# ============================================
# Optional: Kaggle Configuration
# ============================================
# Only needed if running on Kaggle platform
# Set API key through Kaggle Secrets instead

# ============================================
# Model Configuration (Optional)
# ============================================
# Default model for agents
# MODEL_NAME=gemini-2.5-flash-lite

# ============================================
# Development Settings (Optional)
# ============================================
# Enable debug mode
# DEBUG=true

# Log level (DEBUG, INFO, WARNING, ERROR)
# LOG_LEVEL=INFO
